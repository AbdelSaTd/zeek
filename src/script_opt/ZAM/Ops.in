	auto op2 = frame[z.v2].ToVal({NewRef{}, tt_ind[1]});
	ListVal lv(TYPE_ANY);
	lv.Append(op1.release());
	lv.Append(op2.release());
	if ( ! frame[z.v3].table_val->Lookup(&lv) )
		BRANCH(v4)

