internal-op Construct-Table
type VV
eval	IntrusivePtr<TableType> tt = {NewRef{}, z.t->AsTableType()};
	IntrusivePtr<Attributes> attrs = {NewRef{}, z.attrs};
	auto new_t = new TableVal(tt, attrs);
	auto aux = z.aux;
	auto n = aux->n;
	auto ind_width = z.v2;
	for ( auto i = 0; i < n; ++i )
		{
		auto indices = aux->ToIndices(frame, i, ind_width);
		auto v = aux->ToVal(frame, i + ind_width);
		new_t->Assign(indices.get(), v);
		i += ind_width;
		}
	auto& t = frame[z.v1].table_val;
	Unref(t);
	t = new_t;

