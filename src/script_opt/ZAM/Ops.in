op Sync-Globals
type X
eval	for ( auto i = 0; i < num_globals; ++i )
		{
		if ( global_state[i] == GS_DIRTY )
			{
			auto id = globals[i].id;
			auto slot = globals[i].slot;
			auto t = id->Type();
			auto v = frame[slot].ToVal({NewRef{}, t});
			id->SetVal(v);
			}
		global_state[i] = GS_UNLOADED;
		}

