internal-op Next-Table-Iter-Val-Var
# v1 = slot of the "ValueVar"
# v2 = iteration info
# v3 = branch target if loop done
type VVV
eval	HashKey* k;
	auto ii = frame[z.v2].iter_info;
	TableEntryVal* current_tev = ii->loop_vals->NextEntry(k, ii->c);
	if ( ! current_tev )
		BRANCH(v3)
	auto ind_lv = ii->tv->RecoverIndex(k);
	delete k;
	IntrusivePtr<Val> tev_p = {NewRef{}, current_tev->Value()};
	auto val_var_t = ii->value_var_type;
	if ( z.is_managed )
		DeleteManagedType(frame[z.v1]);
	frame[z.v1] = BuildVal(tev_p, val_var_t);
	for ( int i = 0; i < ind_lv->Length(); i++ )
		{
		IntrusivePtr<Val> ind_lv_p = {NewRef{}, ind_lv->Index(i)};
		auto ind = ii->loop_vars[i];
		auto loop_var_t = ii->loop_var_types[i];
		if ( ZVal::IsManagedType(loop_var_t) )
			DeleteManagedType(frame[ind]);
		frame[ind] = BuildVal(ind_lv_p, loop_var_t);
		}

