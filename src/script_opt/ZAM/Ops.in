# Templates used to generate virtual functions, opcodes, and evaluation
# code for compiled code.

########## Unary Ops ##########

assign-op Assign
type V

unary-expr-op Clone
no-const
op-type X
set-type $1
set-type2 $2
eval	auto v = frame[z.v2].ToVal(z.t2)->Clone();
	AssignV1(BuildVal(v, z.t))

unary-expr-op Size
no-const
op-type I U D A N S T V *
explicit-result-type
set-type $1
set-type2 $2
eval-type I	$$.uint_val = $1 < 0 ? -$1 : $1;
eval-type U	$$.uint_val = $1;
eval-type D	$$.double_val = $1 < 0 ? -$1 : $1;
eval-type A	$$.uint_val = $1->AsAddr().GetFamily() == IPv4 ? 32 : 128;
eval-type N	$$.double_val = pow(2.0, double(128 - $1->AsSubNet().LengthIPv6()));
eval-type S	$$.uint_val = $1->Len();
eval-type T	$$.uint_val = $1->Size();
eval-type V	$$.uint_val = $1->Size();
eval	auto v = frame[z.v2].ToVal(z.t2)->SizeVal();
	$$ = BuildVal(v, z.t);

